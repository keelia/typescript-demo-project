{
    "compilerOptions": {
      "target": "es5",
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "strict": true, 
      "skipLibCheck": true, 
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "Node",
      "resolveJsonModule": true
    },
    "exclude": [
      "./test/",
      "./node_modules/",
      "./dist/",
      "./dev-dist/"
    ],
  }